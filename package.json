{
  "name": "mern",
  "version": "1.0.0",
  "description": "Mern Demo",
  "main": "server.js",
  "proxy": "http://0.0.0.0:3001",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "axios": "^0.19.0",
    "bipsms": "^0.4.3",
    "bootstrap": "^4.3.1",
    "express": "^4.17.1",
    "formik": "^1.5.7",
    "if-env": "^1.0.4",
    "install": "^0.12.2",
    "jquery": "^3.4.1",
    "mdbreact": "^4.17.0",
    "moment": "^2.24.0",
    "mysql2": "^1.6.5",
    "popper": "^1.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-moment": "^0.9.2",
    "react-router-dom": "^5.0.1",
    "react-simple-infinite-scroll": "^0.0.1",
    "react-weather": "^0.1.3",
    "reactstrap": "^8.0.0",
    "sendmail": "^1.4.1",
    "sequelize": "^5.8.12",
    "yarn": "^1.16.0"
  }
}
